<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXMLSupprimerSalleDeSportController">
   <children>
      <VBox prefHeight="400.0" prefWidth="134.0" style="-fx-background-color: #8C999B;">
         <children>
            <ImageView fitHeight="129.0" fitWidth="155.0" pickOnBounds="true">
               <image>
                  <Image url="@../images/Capture1.PNG" />
               </image>
            </ImageView>
            <Label prefHeight="39.0" prefWidth="158.0" />
            <Label prefHeight="63.0" prefWidth="158.0" text="     ID SalleDeSport:">
               <font>
                  <Font name="System Bold Italic" size="15.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <TextField fx:id="idSalleDeSport" layoutX="201.0" layoutY="188.0" />
      <Button fx:id="btnSupprimerSS" layoutX="295.0" layoutY="264.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="111.0" text="Supprimer">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
         <effect>
            <InnerShadow>
               <color>
                  <Color red="1.0" green="0.2638888955116272" blue="0.2638888955116272" />
               </color>
            </InnerShadow>
         </effect>
      </Button>
      <Button fx:id="btnRetourSSS" layoutX="164.0" layoutY="264.0" mnemonicParsing="false" onAction="#retourAction" prefHeight="44.0" prefWidth="111.0" text="Retour">
         <font>
            <Font name="System Italic" size="17.0" />
         </font>
         <effect>
            <InnerShadow>
               <color>
                  <Color red="1.0" green="0.7791666388511658" blue="0.2638888955116272" />
               </color>
            </InnerShadow>
         </effect>
      </Button>
      <ImageView fitHeight="143.0" fitWidth="266.0" layoutX="155.0" layoutY="-3.0" pickOnBounds="true">
         <image>
            <Image url="@../images/Capture.PNG" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
